{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "strict": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "useDefineForClassFields": true,
    "baseUrl": "./",
    "outDir": "bundles/",
    "paths": {
      "@tanbo/react-di": [
        "src/public-api.ts"
      ]
    },
    "jsx": "react",
    "jsxFactory": "React.createElement"
  },
  "include": [
    "index.ts",
    "src/",
    "test/"
  ]
}
